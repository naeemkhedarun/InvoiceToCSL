<Window x:Class="InvoiceToCSL.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Window1" Height="300" Width="300">
    <StackPanel>
        <Button Name="browseButton" Click="browseButton_Click">Select File</Button>
        <Button Name="convertFileButton" Click="convertFile_Click" Visibility="Collapsed">Convert</Button>
        <TextBlock Name="outputTextBox"></TextBlock>
        <ProgressBar Name="progressBar" Minimum="0" Maximum="1" Height="20" />
    </StackPanel>
</Window>
